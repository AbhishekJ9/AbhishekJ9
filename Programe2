package company;

import java.util.Scanner;

public class Prograne2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner jj = new Scanner(System.in);
		System.out.println("write a number");
		int num = jj.nextInt();
		int a=0;
		for (int i=0;;i++) {
			if(i%2==1)
			{
			System.out.println(i);	
			a=a+1;
			if(a==num) {
				break;
			}
			}
		}

	}
}
