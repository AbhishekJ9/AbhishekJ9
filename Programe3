package company;

import java.util.Scanner;

public class Programe3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner jj = new Scanner(System.in);
		System.out.println("enter a number");
		int a = jj.nextInt();
		int count = 0;
		if (a % 2 == 0) {
			--a;
		}
		for (int i = 0;; i++) {
			if(i%2==1)
			{
			System.out.println(i);
			count = count + 1;
			if (count == a) {
				break;
			}
			
			
			}
		}

	}

}
